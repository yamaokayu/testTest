{
  "comments": false,

  "plugins": [
    ["@babel/plugin-proposal-object-rest-spread"],
    ["@babel/plugin-transform-async-to-generator"],
    ["babel-plugin-module-resolver", {
      "root": ["scripts"],
      "extensions": [".js", ".jsx"]
    }]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "58",
          "esmodules": true
        }
      }
    ]
  ]
}
