{
  "name": "cocoscoreclient",
  "version": "1.0.0",
  "description": "cocoscoreclient for develop",
  "main": "main.js",
  "scripts": {
    "start": "browser-sync start --server --files * --port 8000",
    "build": "gulp babel",
    "build-web": "gulp babel && node tools/compilation/swapSrcListToDist.js && cocos compile -p web -m release",
    "lint-js": "npm install && eslint --cache .",
    "check": "mocha --require @babel/register",
    "test": "npm install && mocha --require @babel/register",
    "mocha": "NODE_ENV=test mocha",
    "validate": "eslint ."
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-strict-mode": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/plugin-transform-async-to-generator": "7.3.4",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-eslint": "10.0.1",
    "ajv": "^5.5.2",
    "browser-sync": "^2.23.6",
    "commander": "^2.15.1",
    "doctrine": "^2.1.0",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-babel-module": "5.0.1",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsdoc": "3.14.1",
    "eslint-plugin-lodash": "5.1.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-max-comments-per-function": "0.0.2",
    "eslint-plugin-prefer-arrow-in-array": "git+https://github.com/miangraham/eslint-plugin-prefer-arrow-in-array.git#f1cae36c3c749485e5845bb96776c073cb1d50df",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "chai": "^4.2.0",
    "husky": "1.2.1",
    "json2csv": "4.3.2",
    "lint-staged": "8.1.4",
    "nodemon": "1.18.9",
    "acorn": "6.1.0",
    "mocha": "^5.2.0",
    "should": "^8.3.0",
    "prettier": "1.16.4",
    "curl": "0.1.4",
    "http-server": "0.11.1"
  },
  "license": "ISC",
  "dependencies": {
    "google-closure-compiler": "^20180204.0.0"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "dist/**/*",
      "tools/**/*",
      "node_modules/**/*"
    ]
  }
}
